<h1>Dashboard#index</h1>
<p>Find me in app/views/dashboard/index.html.erb</p>
<h2>My Games</h2>
<% current_user.games.each do |game| %>
  <p><%= game.inspect %></p>
<% end %>
<h2><%= link_to("Open a new table", new_game_path)%></h2>
<h2>Other tables</h2>
<% @other_games.each do |game| %>
  <p><%= game.inspect %>
<% end %>
<p><%= link_to current_user.handle, current_user %></p>
<div id="logout_btn"><%= button_to "Log out", session_path, method: :delete %></div>
